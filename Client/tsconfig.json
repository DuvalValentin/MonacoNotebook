{
  "compilerOptions": {
    "target": "esnext",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "ESnext",
    "outDir": "compiledJS",
    "pretty": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "noImplicitReturns": true,
    "declaration": true,
    "sourceMap":true
  }
}
